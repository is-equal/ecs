{
  "compilerOptions": {
    "incremental": true,
    "strict": true,
    "target": "ES6",
    "module": "ES6",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "stripInternal": true,
    "noUncheckedIndexedAccess": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*"],
  "exclude": ["**/lib", "**/node_modules"]
}
